      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}
 
  
<script>
    /* Start  Show User Login Details On Click */
    $(document).ready(function () {
        $('.user-login-icon-click').on('click', function () {
            $('.account-dropdown-container-mob').toggle();
        });
    });
    /* End Show User Login Details On Click */
    function openNav() {
        document.body.style.overflowY = "hidden";
        $("#mobile-menu-nav").animate({
            left: "+=380"
        }, 200, "linear", function () {
            // Animation complete.
        });
        setTimeout(function () {
            $("body").addClass("overlay-bg");
        }, 100);
    }
    function closeNav() {
        $("#mobile-menu-nav").animate({
            left: "-=380"
        }, 200, "linear", function () {
            // Animation complete.
        });
        document.body.style.overflowY = "inherit";
        setTimeout(function () {
            $("body").removeClass("overlay-bg");
        }, 350);
    }
    /* Start Image Preloader Js */
    $(document).ready(function () {
        $.preloadImages = function () {
            for (var i = 0; i < arguments.length; i++) {
                $("<img />").attr("src", arguments[i]);
            }
        }
        $.preloadImages("https://cdn.instasmile.com/new-website/images/h_icon_1_hover.svg", "https://cdn.instasmile.com/new-website/images/h_icon_2_hover.svg", "https://cdn.instasmile.com/new-website/images/h_icon_3_hover.svg", "https://cdn.instasmile.com/new-website/images/h_icon_4_hover.svg", "https://cdn.instasmile.com/new-website/images/h_icon_5_hover.svg", "https://cdn.instasmile.com/new-website/images/h_icon_6_hover.svg", "https://cdn.instasmile.com/new-website/images/h_icon_7_hover.svg", "https://cdn.instasmile.com/new-website/images/icon-instasmile-dynamic-hover.svg", "https://cdn.instasmile.com/new-website/images/h_icon_9_hover.svg");
    });
</script>
    
