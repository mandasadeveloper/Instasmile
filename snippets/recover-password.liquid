<section class="login-details-page page-template login-page">
<section class="login-register-sec">
<div class="container">
<div class="row login-page-row-1">
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 login-clmn-1 commo-form-clmn">
<h1>Forgot your password?</h1>
  
  {%- form 'recover_customer_password' -%}
  {% assign recover_success = form.posted_successfully? %}
  
<div class="form-row">
<div class="form-group col-lg-12" style="margin-bottom: 10px;">
<div class="form_label"><label for="Email">Email address</label></div>
<input type="email"  class="form-control input-field"
          value=""
          name="email"
          id="RecoverEmail"
          autocorrect="off"
          autocapitalize="off"
          autocomplete="email"
          {% if form.errors %}
            aria-invalid="true"
            aria-describedby="RecoverEmail-email-error"
            autofocus 
          {% endif %}
          placeholder=""
        >
  
  </div>
  
   {%- if form.errors -%}
         <small style="color: #dc3545!important; font-size: 15px;"> The email address you entered doesn't exist.</small>
      {%- endif -%}
</div>
<div class="form-row">
<div class="form-group col-lg-12 field-correct form-btn-common login-form-btn">
<button class="btn" type="submit">Reset Password</button>
</div>
</div>
  {% endform %}

</div>
</div>
</div>
</section>
