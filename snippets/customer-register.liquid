<section class="login-details-page page-template login-page">
<section class="login-register-sec">
<div class="container">
<div class="row login-page-row-1 row-eq-height">
<div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 login-clmn-1">
<h1>Login to your account</h1>
<div class="green-btn-login"><a class="btn btn-primary" href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" role="button">Login with email</a></div>

</div>
<div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 login-clmn-1 commo-form-clmn">
<h2>Create a new account</h2>
  
  
   {%- form 'create_customer', novalidate: 'novalidate' -%}
    {%- if form.errors -%}
      <small class="form__message" tabindex="-1" autofocus>
        {{ 'templates.contact.form.error_heading' | t }}
      </small>
      <ul> 
        {%- for field in form.errors -%}
          <li>
            {%- if field == 'form' -%}
              {{ form.errors.messages[field] }}
            {%- else -%}
              <a href="#RegisterForm-{{ field }}">
                {{ form.errors.translated_fields[field] | capitalize }}
                {{ form.errors.messages[field] }}
              </a>
            {%- endif -%}
          </li>
        {%- endfor -%}
      </ul>
    {%- endif -%}
  
  <div class="form-row">
<div class="form-group col-lg-12">
<div class="form_label"><label for="Name">Your name</label></div>
<input type="text" class="form-control input-field" required
        name="customer[first_name]"
        id="RegisterForm-FirstName"
        autocomplete="given-name"
        placeholder="" data-val="true" data-val-required="The Name field is required." value="">
  <span class="text-danger field-validation-valid" data-valmsg-for="Name" data-valmsg-replace="true"></span>
</div>
<div class="form-group col-lg-12">
<div class="form_label"><label for="Email">Email address</label></div>
 <input
        type="email" class="form-control input-field" placeholder="" data-val="true" data-val-email="The Email field is not a valid e-mail address." data-val-required="The Email field is required."
        name="customer[email]" required
        id="email"
        {% if form.email %} value="{{ form.email }}"{% endif %}
        spellcheck="false"
        autocapitalize="off"
        autocomplete="email"
        aria-required="true"
        {% if form.errors contains 'email' %}
          aria-invalid="true"
          aria-describedby="RegisterForm-email-error"
        {% endif %}>
  <span class="text-danger field-validation-valid" data-valmsg-for="Email" data-valmsg-replace="true"></span>
</div>
</div>
  <div class="form-row">
<div class="col-lg-12">
<h3 class="login_form_h3">Password</h3>
<p>Passwords must contain at least one uppercase character and a number</p>
</div>
</div>
<div class="form-row">
<div class="form-group col-lg-12">
<div class="form_label"><label class="control-label" for="Password">Password</label></div>
  <input
        type="password" class="form-control input-field" data-val="true" data-val-required="The Password field is required." 
        name="customer[password]" required
        id="Password"
        aria-required="true"
        {% if form.errors contains 'password' %}
          aria-invalid="true"
          aria-describedby="RegisterForm-password-error"
        {% endif %}>
  
  <span class="text-danger field-validation-valid" data-valmsg-for="Password" data-valmsg-replace="true"></span>
</div>
<div class="form-group col-lg-12">
<div class="form_label"><label class="control-label" for="ConfirmPassword">Confirm Password</label></div>
<input type="password" class="form-control input-field" data-val="true" data-val-equalto="&#x27;Confirm Password&#x27; and &#x27;Password&#x27; do not match." data-val-equalto-other="*.Password" data-val-required="The Confirm Password field is required." id="ConfirmPassword"
        name="customer[ConfirmPassword]"
        aria-required="true" required
        {% if form.errors contains 'password' %}
          aria-invalid="true"
          aria-describedby="RegisterForm-password-error"
        {% endif %}>
  
  <span class="text-danger field-validation-valid" data-valmsg-for="ConfirmPassword" data-valmsg-replace="true"></span>
<asp:CompareValidator ID="ConfirmPasswordValidator" runat="server" ControlToCompare="Password" ControlToValidate="ConfirmPassword" ErrorMessage="not match">
</div>
</div>
  
  <div class="form-row">
<div class="single-checkbox">
<div class="check-box-wrap">
<input type="checkbox" id="login_subscribe" name="customer[LoginSubscribe]" value="login_subscribe" data-val="true" data-val-required="The ContactPreferences field is required.">
<label for="login_subscribe" required>I’d like Instasmile to send me offers, discounts and new product information. I can opt out at any time.</label>
</div>
</div>
<span class="text-danger field-validation-valid" data-valmsg-for="ContactPreferences" data-valmsg-replace="true"></span>
</div>
<div class="form-row">
<div class="form-group col-lg-12 field-correct form-btn-common login-form-btn">
<button type="submit" value="Continue">Continue</button>
</div>
</div>
  
  {% endform %}
  
  
