<script>
  $('#submitArch').on('submit',function(e){
    e.preventDefault();
    var form_data = $(this).serializeArray();
      console.log(form_data);
    var object = {};
    object['UpperMissingTeeth'] = "";
    object['LowerMissingTeeth'] = "";
   
    form_data.forEach(function(value, key){
      var value_v = value.name;
      var object_key = object[key];
     
      if(isNaN(value.value)){
      object[value.name] = value.value;
      }else{
        if(value_v.includes("UpperMissingTeeth")){
            object['UpperMissingTeeth'] += value.value +",";
        }else if(value_v.includes("LowerMissingTeeth")){
            object['LowerMissingTeeth'] += value.value +",";
        
        }
      }
    });
    var json = JSON.stringify(object);
     console.log(json);
    $.cookie("form_data",json);
    
    location.href='/pages/products'
  });
</script>