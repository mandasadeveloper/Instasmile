<section class="login-details-page page-template login-page">
<section class="login-register-sec">
<div class="container">
<div class="row login-page-row-1">
 <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 login-clmn-1 commo-form-clmn">
   
<h1 id="login" tabindex="-1">Login to your account</h1>
   
    <div>
    {%- if recover_success == true -%}
      <h3 class="form__message" tabindex="-1" autofocus>
        <svg aria-hidden="true" focusable="false" role="presentation" viewBox="0 0 13 13">
          <path d="M6.5 12.35C9.73087 12.35 12.35 9.73086 12.35 6.5C12.35 3.26913 9.73087 0.65 6.5 0.65C3.26913 0.65 0.65 3.26913 0.65 6.5C0.65 9.73086 3.26913 12.35 6.5 12.35Z" fill="#428445" stroke="white" stroke-width="0.7"/>
          <path d="M5.53271 8.66357L9.25213 4.68197" stroke="white"/>
          <path d="M4.10645 6.7688L6.13766 8.62553" stroke="white">
        </svg>
        {{ 'customer.recover_password.success' | t }}
      </h3>
    {%- endif -%}
        </div>
   
{%- form 'customer_login', novalidate: 'novalidate' -%}
   
<div class="form-row">
<div class="form-group col-lg-12">
<div class="form_label"><label for="Email">Email address</label></div>
  <input type="email" class="form-control input-field" name="customer[email]"  id="CustomerEmail"
          autocomplete="email"
          autocorrect="off"
          autocapitalize="off"
          placeholder="" required="required">
  </div>
  
<div class="form-group col-lg-12">
<div class="form_label"><label for="Password">Password</label></div>
 <input  type="password" class="form-control input-field"
            value=""
            name="customer[password]"
            id="Password"
            placeholder=""
         required="required"> 
  </div>  
</div>
          
 <a href="/pages/recover-password" class="text-right text-muted">Forgot your password?</a>
          
<div class="form-row">
<div class="form-group col-lg-12 field-correct form-btn-common login-form-btn">
<button class="btn" type="submit">Continue</button>
</div>
</div>
                    
  {% endform %}
   
   
   

</div>
<div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 login-clmn-1">
<h2>Create a new account</h2>
<div class="green-btn-login"><a class="btn btn-primary" href="{{ routes.account_register_url }}" role="button">Create a new account</a></div>
</div>
</div>
  
   {%- if shop.checkout.guest_login -%}
    <div>
      <hr>
      <h2>{{ 'customer.login_page.guest_title' | t }}</h2>

      {%- form 'guest_login' -%}
        <button>
          {{ 'customer.login_page.guest_continue' | t }}
        </button>
      {%- endform -%}
    </div>
  {%- endif -%}
  
</div>
</section>

